(install
  (files (arith_flags.cmx as ocaml/arith_flags.cmx)
         (arith_status.cmi as ocaml/arith_status.cmi)
         (arith_status.cmx as ocaml/arith_status.cmx)
         (arith_status.mli as ocaml/arith_status.mli)
         (big_int.cmi as ocaml/big_int.cmi)
         (big_int.cmx as ocaml/big_int.cmx)
         (big_int.mli as ocaml/big_int.mli)
         (int_misc.cmx as ocaml/int_misc.cmx)
         (nat.cmi as ocaml/nat.cmi)
         (nat.cmx as ocaml/nat.cmx)
         (nat.mli as ocaml/nat.mli)
         (num.cmi as ocaml/num.cmi)
         (num.cmx as ocaml/num.cmx)
         (num.mli as ocaml/num.mli)
         (nums%{ext_lib} as ocaml/nums%{ext_lib})
         (nums.cma as ocaml/nums.cma)
         (nums.cmxa as ocaml/nums.cmxa)
         (nums.cmxs as ocaml/nums.cmxs)
         (libnums%{ext_lib} as ocaml/libnums%{ext_lib})
         (dllnums%{ext_dll} as ocaml/stublibs/dllnums%{ext_dll})
         (ratio.cmi as ocaml/ratio.cmi)
         (ratio.cmx as ocaml/ratio.cmx)
         (ratio.mli as ocaml/ratio.mli))
  (section lib_root))

(rule (copy %{cmx:arith_flags} arith_flags.cmx))
(rule (copy %{cmi:arith_status} arith_status.cmi))
(rule (copy %{cmx:arith_status} arith_status.cmx))
(rule (copy %{cmi:big_int} big_int.cmi))
(rule (copy %{cmx:big_int} big_int.cmx))
(rule (copy %{cmx:int_misc} int_misc.cmx))
(rule (copy %{cmi:nat} nat.cmi))
(rule (copy %{cmx:nat} nat.cmx))
(rule (copy %{cmi:num} num.cmi))
(rule (copy %{cmx:num} num.cmx))
(rule (copy %{cmi:ratio} ratio.cmi))
(rule (copy %{cmx:ratio} ratio.cmx))

